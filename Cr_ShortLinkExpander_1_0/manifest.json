{
  "name": "Short Link Expander",
  "version": "1.0",
  "description": "Short Link Expander",
  "permissions": ["http://4.expndr.appspot.com/*"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["expand.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background_page": "background.html",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}
